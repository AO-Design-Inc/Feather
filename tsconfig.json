{
	"compilerOptions": {
		"esModuleInterop": false,
		"outDir": "built",
		"downlevelIteration": true,
		"module": "ESNext",
		"experimentalDecorators": true,
		"lib": ["es2019"],
		"target": "es6",
		"strict": true,
		"alwaysStrict": true,
		"strictNullChecks": true
	},
	"typedocOptions": {
		"entryPoints": "src",
		"out": "docs",
		"name": "Feather"
	},
	"include": ["src/**/**/*"],
	"exclude": ["node_modules", "**/*.spec.ts"]
}
